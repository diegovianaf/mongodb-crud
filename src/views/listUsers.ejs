<!DOCTYPE html>
<html lang="en">
<%- include('partials/head.ejs') %>
<body>
  <%- include('partials/navbar.ejs') %>

  <h1><%= title %> </h1>

  <ul>
    <% users.forEach((user) => { %>
      <li>
        <%= user.name %> - 
        age: <%= user.age %> -
        <%= user.email %> 
        <a href="/update?id=<%= user._id %>">[edit]</a>
      </li>
    <% }) %>
  </ul>

  <%- include('partials/foot.ejs') %>
</body>
</html>