<!DOCTYPE html>
<html lang="en">
<%- include('partials/head.ejs') %>
<body>
  <%- include('partials/navbar.ejs') %>

  <h1><%= title %> </h1>

  <ul>
    <% users.forEach((user) => { %>
      <li style="margin-bottom: 12px;">
        <%= user.name %> - 
        age: <%= user.age %> -
        <%= user.email %> -
        <a href="/update?id=<%= user._id %>">update</a> |
        <a href="/remove/<%= user._id %>">remove</a>
      </li>
    <% }) %>
  </ul>

  <%- include('partials/foot.ejs') %>
</body>
</html>